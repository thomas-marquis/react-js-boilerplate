{
  "name": "react-js-boilerplate",
  "version": "1.0.0",
  "description": "Simple Reactjs boilerplate with emotions and material-ui. Redux or unit tests are not configured.",
  "main": "index.js",
  "scripts": {
    "buil:prod": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "run:node": "node app.js",
    "run:prod": "npm run build:prod && npm run run:node",
    "run:dev": "npm run build:dev && npm run run:node",
    "start:dev": "webpack-dev-server --hot"
  },
  "keywords": [
    "react",
    "js",
    "node",
    "ejs",
    "webpack",
    "boilerplate",
    "express",
    "emotion",
    "material-ui"
  ],
  "author": "Thomas Marquis",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^3.6.2",
    "axios": "^0.18.0",
    "ejs": "^2.6.1",
    "emotion": "^10.0.5",
    "express": "^4.16.4",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-emotion": "^10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^2.0.1",
    "ejs-loader": "^0.3.1",
    "eslint": "^6.8.0",
